{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "root": true,
    "rules": {
        "react/react-in-jsx-scope": "off",
        "@typescript-eslint/space-before-function-paren": "off",
        "@typescript-eslint/no-unused-vars": "warn"
    }
}